<% if (chat) { %>
<div class="py-1" >
  <div class="px-3 py-2 d-flex justify-content-between border-bottom">
    <span class="me-2" data-member-num-chat-id="<%= chatId %>">MEMBER<%= group.members.length == 1 ? '' : 'S' %>-<%=group.members.length%></span>
  </div>
  <div class="group-chat-list p-2 pb-0 text-primary">
    <ul class="list-unstyled p-2" data-member-chat-id="<%= chatId %>">
      <% for(let i=0; i<group.members.length; i++) {%>
      
        <li class="mt-3">
          <%= group.members[i].userName%>
        </li>
      
      <% } %>
    </ul>
  </div>
</div>
<% } %>