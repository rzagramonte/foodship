<% if (chat) { %>
<div class="container flex-grow-1 p-1">
  <div class="p-2 pb-2 border-bottom border-secondary">
    <span class="me-2 ">MEMBERS-<%=group.members.length%></span>

  </div>

  <div class="group-chat-list list-group p-2 pb-0 text-primary">

    <ul class="list-unstyled ">
      <% for(let i=0; i<group.members.length; i++) {%>
      <!--make the this a lil cute box-->
      <div class="d-flex">
        <li class="mt-3">
          <%= group.members[i].userName%>
        </li>
      </div>
      <% } %>
    </ul>

  </div>
</div>

</div>
<% } %>